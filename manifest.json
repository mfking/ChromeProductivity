{

 "name": "My Daily Planner",
 "description": "A daily planner that helps you stay productive",
 "version": "1.0.0",
 "manifest_version": 2,

"content_security_policy": "script-src 'self' https://www.google-analytics.com https://*.yahooapis.com; object-src 'self'",
"content_scripts": [
	{
		"matches": [ "<all_urls>" ],
		//"css": [],
		//"js": [ "/js/jquery/jquery-1.11.0.min.js", "js/in.js", "js/core.js" ]
	}
],

 "icons": {
    "128": "images/icon.png"
 },

 "permissions": [
    "history"
  ],

  "chrome_url_overrides" : {
    "newtab": "productivity.html"
  }
}